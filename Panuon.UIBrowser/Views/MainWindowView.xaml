<Window x:Class="Panuon.UIBrowser.Views.MainWindowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:partial="clr-namespace:Panuon.UIBrowser.Partial"
        xmlns:pu="clr-namespace:Panuon.UI;assembly=Panuon.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <ScrollViewer>
            <StackPanel>
                <partial:Buttons></partial:Buttons>
                <Grid Height="70"></Grid>
                <partial:TextBoxs></partial:TextBoxs>
                <Grid Height="70"></Grid>
                <partial:CheckBoxs></partial:CheckBoxs>
                <Grid Height="70"></Grid>
                <partial:RadioButtons></partial:RadioButtons>
                <Grid Height="70"></Grid>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Window>
